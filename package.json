{
    "name": "vite-plugin-decap-cms",
    "version": "0.4.0",
    "description": "Simplify the configuration of Decap cms for Vite projects",
    "type": "module",
    "license": "MIT",
    "keywords": [
        "vite",
        "vite-plugin",
        "decap-cms"
    ],
    "homepage": "https://github.com/ghostrider-05/vite-plugin-decap-cms",
    "bugs": "https://github.com/ghostrider-05/vite-plugin-decap-cms/issues",
    "repository": {
        "type": "git",
        "url": "https://github.com/ghostrider-05/vite-plugin-decap-cms"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --dts --format cjs,esm"
    },
    "overrides": {
        "react": "^18.3.1",
        "@types/react": "^18.3.4",
        "react-dom": "^18.3.1"
    },
    "dependencies": {
        "decap-cms-core": "^3.4.2",
        "decap-server": "^3.1.2",
        "veaury": "^2.4.3",
        "yaml": "^2.5.0"
    },
    "devDependencies": {
        "@types/node": "18",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^8.57.0",
        "tsup": "^8.2.4",
        "typescript": "^5.5.4",
        "vite": "^5.4.2",
        "vitest": "^2.0.5",
        "vue": "^3.4.38"
    }
}
